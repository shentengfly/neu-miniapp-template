{
	"pages": [
		{
			"path": "pages/index",
			"style": {
				// The title can be configured as a dynamic variable. Use %% to represent the variable.
				"navigationBarTitleText": "%title.home%"
			}
		},
		{
			"path": "pages/list",
			"style": {
				"navigationBarTitleText": "%title.list%"
			}
		}
	],
	// Global configuration.
	"globalStyle": {
		"navigationBarTextStyle": "black", // Text color of navigation bar and notification bar.
		"navigationBarTitleText": "Home",
		"navigationBarBackgroundColor": "#F8F8F8", // Background color of navigation bar.
		"backgroundColor": "#F8F8F8"
	},
	// Tab bar configuration.
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#2071fa",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/index",
				"text": "Home",
				"iconPath": "static/tabbar/home.png",
				"selectedIconPath": "static/tabbar/home-active.png"
			},
			{
				"pagePath": "pages/list",
				"text": "List",
				"iconPath": "static/tabbar/star.png",
				"selectedIconPath": "static/tabbar/star-active.png"
			}
		]
	}
}